{
  "version": 2,
  "alias": "johnapostol.com",
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ],
  "headers": [
    {
      "source": "/_next/static/(?:[^/]+/pages|chunks|runtime)/.+",
      "headers": [
        { 
          "key": "Cache-Control",
          "value": "immutable, max-age=31536000"
        }
      ]
    }
  ]
}
