{
  "version": 2,
  "alias": "johnapostol.com",
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(?:[^/]+/pages|chunks|runtime)/.+",
      "headers": { "cache-control": "immutable,max-age=31536000" }
    }
  ],
  "env": {
    "baseUrl": "@now_url"
  }
}
